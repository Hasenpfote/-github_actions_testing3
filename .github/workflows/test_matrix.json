{
  "os": [
    "ubuntu-20.04",
    "macos-latest",
    "windows-latest",
  ],
  "python-version": [
    "3.7", "3.8",
  ],
  "include": [
    {
      "os": "ubuntu-20.04",
      "poetry_version": "1.3.1",
      "poetry_home": "",
      "poetry_path": "",
      "poetry_cache_paths": "~/.local/share/pypoetry\n~/.local/bin/poetry",
    },
    {
      "os": "macos-latest",
      "poetry_version": "1.3.1",
      "poetry_home": "",
      "poetry_path": "${HOME}/.local/bin",
      "poetry_cache_paths": "~/Library/Application Support/pypoetry\n~/.local/bin/poetry",
    },
    {
      "os": "windows-latest",
      "poetry_version": "1.3.1",
      "poetry_home": "",
      "poetry_path": "${APPDATA}\\Python\\Scripts",
      "poetry_cache_paths": "~\\AppData\\Roaming\\pypoetry\n~\\AppData\\Roaming\\Python\\Scripts\\poetry.exe",
    },
  ],
  "poetry_cache_key_fmt": [
    "poetry-{0}-{1}-python-{2}",
  ],
  "venv_cache_key_fmt": [
    "venv-{0}-python-{1}-{2}",
  ],
}
